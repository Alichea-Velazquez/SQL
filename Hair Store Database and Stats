SELECT * FROM hairstore;
SELECT * FROM hairstore ORDER BY hairtype;
SELECT * FROM hairstore ORDER BY price;
SELECT * FROM hairstore WHERE price >25 ORDER BY price;
SELECT SUM (quantity) from hairstore GROUP BY hairtype;
SELECT hairtype, SUM (quantity)FROM hairstore GROUP BY name;
